#include <stdlib.h>
#include <stdio.h>
#include "hash_table.h"

int main( void )
{
    HashTable* ht = malloc( sizeof( HashTable ) );

    int art = 3;

    ht_init( ht, 20 );


    ht_add( ht, "ABCD", &art );
    ht_add( ht, "OBCJDK", &art );
    ht_add( ht, "OBCJDK", &art );
    ht_add( ht, "OBCJDK", &art );
    ht_add( ht, "OBCJDK", &art );
    ht_add( ht, "OBCJDK", &art );
    ht_add( ht, "OBCJDK", &art );

    printf( "%d\n", *(int*)ht_find( ht, "ABCD" ) );
    ht_delete( ht, "ABCD" );
    printf( "%d\n", *(int*)ht_find( ht, "OBCJDK" ) );
    ht_clear( ht );


    return EXIT_SUCCESS;
}
